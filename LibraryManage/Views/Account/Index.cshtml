@using LibraryManage.Models
@{
    ViewBag.Title = "Account";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Account</h2>

@{
    foreach (var data in Model)
    {
        <ul>
            <li>@data.Username<li>
            <li>@data.Password</li>
            <li>@data.ID_Users</li>
            <li>@data.First_Name</li>
            <li>@data.Last_Name</li>
            <li>@data.Type</li>
            <li>@data.Date_of_Birth</li>
            <li>@data.Notes</li>
        </ul>
    }

}